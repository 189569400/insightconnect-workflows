            "continueOnFailure": false,
            "isDisabled": false,
            "parameters": {
              "input": {
                "function": "def run(params={}):\n    # Sometimes Slack will give odd unicode spaces\n    email = params.get(\"email\",\"\")\n    email = email.replace(u\"\\xa0\", u\" \")\n    email = email.split(\" \")[1]\n\n    # Sometimes Slack will give us email in their linked form:\n    # <mailto:user@example.com>\n    # or\n    # <http://www.exmaple.com|example.com>\n    ret_obj = {\"parsed_email\": email}\n    if email.startswith(\"<\"):\n        # mailto link\n        if \"mailto:\" in email:\n            split_email = email.split(\":\")[1]\n            if \"|\" in split_email:\n                ret_obj[\"parsed_email\"] = split_email.split(\"|\")[0]\n\n        # http link\n        elif \"http:\" in email:\n            split_email = email.split(\"|\")[1]\n            split_email = split_email.rstrip(\">\")\n            ret_obj[\"parsed_email\"] = split_email\n\n    return ret_obj\n",
